        <!-- End Address Section -->
      
          <!-- order tracking section -->
        <% orderDetails.products.forEach((product, index) => { %>
            <div class="container">
                <article class="card">
                    <header class="card-header"> My Orders / Tracking </header>
                    <div class="card-body">
                        <h6>Order ID: <%= orderDetails._id %></h6>
                        <article class="card">
                            <div class="card-body row">
                                <div class="col">
                                    <strong>Estimated Delivery time:</strong><br>
                                    <%= new Date(orderDetails.estimatedDeliveryDate).toLocaleDateString('en-US', { day: '2-digit', month: 'short', year: 'numeric' }) %>
                                </div>
                                <div class="col">
                                    <strong>Shipping BY:</strong><br>
                                    BLUEDART, | <i class="fa fa-phone"></i> +1598675986
                                </div>
                                <div class="col">
                                    <strong>Status:</strong><br>
                                    <%= product.orderstatus %>
                                </div>
                                <div class="col">
                                    <strong>Tracking #:</strong><br>
                                    BD045903594059
                                </div>
                            </div>
                        </article>
                        <% if (!(orderDetails.modeOfPayment === 'onlinepay' && orderDetails.paymentStatus === 'pending')) { %>

                        <% if(product.orderstatus=='RequestedReturn' || product.orderstatus === 'Return Accepted' || product.orderstatus === 'Returned') {%>

                            <div class="track" data-track-product-id="<%= product._id %>">
                                <div class="step return-requested-step <%= (product.orderstatus === 'RequestedReturn' || product.orderstatus === 'Return Accepted' || product.orderstatus === 'Returned') ? 'active' : '' %>">
                                    <span class="icon"><i class="fa fa-exchange"></i></span>
                                    <span class="text">Return Requested</span>
                                </div>

                                <div class="step returned-step <%= ( product.orderstatus === 'Return Accepted' || product.orderstatus === 'Returned' ) ? 'active' : '' %>">
                                    <span class="icon"><i class="fa fa-undo"></i></span>
                                    <span class="text">Returned Accepted</span>
                                </div>

                                <div class="step returned-step  <%= (  product.orderstatus === 'Returned' ) ? 'active' : '' %> ">
                                    <span class="icon"><i class="fa fa-undo"></i></span>
                                    <span class="text">Returned</span>
                                </div>
                            </div>

                        <%} else {%>
                       
                        <div class="track" data-track-product-id="<%= product._id %>">
                            <div class="step <%= (product.orderstatus === 'pending' || product.orderstatus === 'Shipped' || product.orderstatus === 'Delivered' ) ? 'active' : '' %>">
                                <span class="icon"><i class="fa fa-check"></i></span>
                                <span class="text">Order confirmed</span>
                            </div>
                            <div class="step <%= (product.orderstatus === 'Shipped' || product.orderstatus === 'Delivered' ) ? 'active' : '' %>">
                                <span class="icon"><i class="fa fa-check"></i></span>
                                <span class="text">Shipped</span>
                            </div>
                            <div class="step <%= (product.orderstatus === 'Delivered' ) ? 'active' : '' %>">
                                <span class="icon"><i class="fa fa-check"></i></span>
                                <span class="text">Delivered</span>
                            </div>
                            <% if (product.orderstatus === 'Cancelled') { %>
                                <div class="step cancelled-step active">
                                    <span class="icon"><i class="fa fa-times"></i></span>
                                    <span class="text">Cancelled</span>
                                </div>
                                <% } %>
                        </div>
                        <%}%>
                        <%} %>
                        <hr>
                        <ul class="row">
                            <li class="col-md-4" data-product-id="<%= product._id %>" data-quantity="<%= product.quantity %>">
                                <figure class="itemside mb-3">
                                    <div class="aside">
                                        <img src="<%- product.variantId.images[0] %>" class="img-sm border">
                                    </div>
                                    <figcaption class="info align-self-center">
                                        <p class="title">
                                            <%= product.productName %> <br>
                                            size: <%= product.variantSize %> <br>
                                            color: <%= product.variantColor %> <br>
                                            count: <%= product.quantity%>
                                        </p>
                                        <span class="text-muted">₹<%= product.variantPrice %></span>
                                        <!-- <p class="status-text"><strong>Status:</strong> <%= product.orderstatus %></p> -->
                                    </figcaption>
                                </figure>
                            </li>
                        </ul>
                        <hr>
                        
                        <% if (!(orderDetails.modeOfPayment === 'onlinepay' && orderDetails.paymentStatus === 'pending')) { %>
                        
                        <%if(product.orderstatus!== 'RequestedReturn' && product.orderstatus !=='Return Accepted' ){%>
                        <% if (product.orderstatus === 'Delivered') { %>
                            <%if(product.cancelOrderStatus==="" ){%>
                            <a  class="btn btn-success return-button" onclick="returnProduct('<%= orderDetails._id %>', '<%= product._id %>','<%= product.quantity %>','<%=product.variantSize%>')" data-product-id="<%= product._id %>">Return</a>
                            <a href="javascript:void(0)" class="btn btn-primary" onclick="downloadInvoice('<%= orderDetails._id %>','<%=product._id%>')">Download Invoice</a>


                            <%}else {%>
                                <p><%=product.cancelOrderStatus%></p>
                                <a href="javascript:void(0)" class="btn btn-primary" onclick="downloadInvoice('<%= orderDetails._id %>','<%=product._id%>')">Download Invoice</a>
                            <%}%>
                        <% } else { %>
                            <%if(product.orderstatus!== 'Returned'){%>
                            <a class="btn btn-danger cancel-button <%= product.orderstatus === 'Cancelled' ? 'disabled' : '' %>" id="cancel-<%=product._id%>" data-abc="true" onclick="cancelProduct('<%= orderDetails._id %>', '<%= product._id %>','<%= product.quantity %>','<%=product.variantSize%>')" <%= product.orderstatus === 'Cancelled' ? 'disabled' : '' %>><%= product.orderstatus === 'Cancelled' ? 'Cancelled' : 'Cancel' %></a>
                            <%}%>
                        <% } %>
                        <%}%>
                        <a href="/orders" class="btn btn-warning" data-abc="true">
                            <i class="fa fa-chevron-left"></i> Back to orders
                        </a>
                      
                        <% } else { %>
                            <!-- Message and Repayment button for online payment pending status -->
                            <div class="alert alert-warning" role="alert">
                                Payment didn't complete. Please click "Repay" to complete your payment.
                            </div>
                           

                            <a class="btn btn-primary repayment-button" 
                            data-product-id="6687dd20105f3184f0b37c46" 
                            data-orderId = "<%=orderDetails._id%>"
                            data-shipping-address-id="<%= orderDetails.shippingAddress._id %>" 
                            data-payment-mode="onlinepay" 
                            data-coupon='<%- JSON.stringify(orderDetails.coupon) %>'
                            onclick="repayProduct(this)">
                            Repay
                            </a>

                        <% } %>

                    </div>
                </article>
            </div>
        <% }) %>

        <!-- Order tracking section -->

        <script>
            function downloadInvoice(orderId,singorderId) {
                fetch(`/download-invoice?orderId=${orderId}&productOrderId=${singorderId}`, {
                    method: 'GET',
                    headers: {
