            async function cancelProduct(orderId, productId, quantity, variantSize) {
               
                try {
                    const result = await Swal.fire({
                        title: "Are you sure?",
                        text: "You won't be able to revert this!",
                        icon: "warning",
                        showCancelButton: true,
                        confirmButtonColor: "#3085d6",
                        cancelButtonColor: "#d33",
                        confirmButtonText: "Yes, cancel order!"
                    });
        
                    if (result.isConfirmed) {
                        const response = await fetch('/cancel-product', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                orderId,
                                productId,
                                quantity,
                                variantSize,
                                status: 'cancelled'
                            })
                        });
        
                        if (response.ok) {
                            const productElement = document.querySelector(`[data-product-id="${productId}"]`);
                            const trackElement = document.querySelector(`[data-track-product-id="${productId}"]`);
        
                            if (productElement && trackElement) {
                                // Update the product status to 'Cancelled'
                                const statusText = productElement.querySelector('.status-text');
                                if (statusText) {
                                    statusText.innerText = 'Cancelled';
                                }
        
                                // Hide all steps except 'Cancelled'
                                trackElement.querySelectorAll('.step').forEach(step => {
                                    step.classList.remove('active');
                                });
                                
                                let cancelledStep = trackElement.querySelector('.cancelled-step');
                                if (!cancelledStep) {
                                    // If cancelled-step is not already present, create and insert it
                                    cancelledStep = document.createElement('div');
                                    cancelledStep.className = 'step cancelled-step active';
                                    cancelledStep.innerHTML = `
                                        <span class="icon"><i class="fa fa-times"></i></span>
                                        <span class="text">Cancelled</span>
                                    `;
                                    trackElement.appendChild(cancelledStep);
                                } else {
                                    cancelledStep.style.display = 'block';
                                    cancelledStep.classList.add('active');
                                }
        
                                // Disable the cancel button and change its text
                                const cancelButton = document.getElementById(`cancel-${productId}`);
                                if (cancelButton) {
                                    cancelButton.disabled = true;
                                    cancelButton.innerText = 'Cancelled';
                                    cancelButton.classList.add('disabled');
                                }
        
                                // Show success message
                                Swal.fire({
                                    title: 'Cancelled!',
                                    text: 'Order cancelled successfully!',
                                    icon: 'success'
                                }).then((result)=>{
                                    if(result.isConfirmed){
                                        window.location.reload()
                                    }
                                })
                            }
                        } else {
                            const errorMessage = await response.json();
                            Swal.fire('Error!', errorMessage.message || 'Failed to cancel order.', 'error');
                        }
                    }
                } catch (error) {
                    console.error('Error:', error);
                    Swal.fire('Error!', 'An error occurred. Please try again.', 'error');
                }
            }
        
        


            async function returnProduct(orderId, productId, quantity, variantSize) {

                console.log('this si the ',orderId,productId,quantity,variantSize)
            try {
            // Step 1: Ask for the return reason
            const { value: returnReason } = await Swal.fire({
                title: 'Return Product',
                input: 'select',
                inputOptions: {
                    'Wrong item': 'Wrong item',
                    'Damaged item': 'Damaged item',
                    'Quality issue': 'Quality issue',
                    'Other': 'Other'
                },
